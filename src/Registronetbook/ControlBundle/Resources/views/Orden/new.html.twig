{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Orden creation</h1>
    <div class="col-md-12 well">

    {{ form_row(form.finalizado) }}
    {{ form_row(form.maquina) }}
    </div>

    <h3>Movimientos</h3>
    <div class="col-md-3 well">
        <ul class="detalleMovimientolista">
            {# iterate over each existing tag and render its only field: name #}
            {% for tag in form.detalleMovimiento %}
                <li>{{ form_row(tag) }}</li>
            {% endfor %}
        </ul>
        <ul class="detalleMovimiento" data-prototype="{{ form_widget(form.detalleMovimiento.vars.prototype)|e }}">
        </ul>
    </div>


    <ul class="record_actions">
        <li>
            {{form_row(form.save)}}
        </li>
    <li>
        <a href="{{ path('admin_orden') }}">
            Back to the list
        </a>
    </li>
</ul>
<script type="text/javascript">

</script>

{% endblock %}
